@model IEnumerable<Inmobiliaria_.Net_Core.Models.Propietario>

@{
    ViewData["Title"] = "Lista de Propietarios";
}

<h2>Lista de Propietarios</h2>

<p>
    <a asp-action="Crear" class="btn btn-primary">Nuevo Propietario</a>
</p>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>DNI</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.IdPropietario</td>
            <td>@p.Nombre</td>
            <td>@p.Apellido</td>
            <td>@p.Dni</td>
            <td>@p.Email</td>
        </tr>
    }
    </tbody>
</table>
